<%= render "shared/evaluation_tab", active_tab: Settings.tabs.rank %>
<div id="ranks">
  <div class="row">
    <div class="col-md-12 title">
      <%= t "layouts.admin.roles" %>
      <span>
        <%= link_to t("rank.buttons.new"),
            new_admin_rank_path, class: "btn btn-success pull-right" %>
      </span>
    </div>
  </div>
  <table id="rank-tbl" class="table table-bordered display" data-source="<%= admin_ranks_url(format: "json") %>" >
    <thead>
      <tr class="active">
        <th></th>
        <th class="rank"><%= t "table.thead.rank" %></th>
        <th class="begin-point"><%= t "table.thead.begin_point" %></th>
        <th class="end-point"><%= t "table.thead.end_point" %></th>
        <th class="action"><%= t "table.thead.action" %></th>
        <th class="action"><%= t "table.thead.action" %></th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>
